			string prefix = "skillet_of_";
			string i = string_trimleft(fixhand, prefix.length());
			if (i == "") return;
			int u = i.find("_");
			if (u <= 0) return;
			string amstr = i.substr(0, u);
			if (!amstr.is_digits()) return;
			double am = stn(amstr);
			if (am < 1) return;
			string fi = i.substr(u + 1, i.length());
			if (fi == "") return;
			p.inv_add_item(fi, am);
			send_packet(6, "play getskillet.ogg " + p.x + " " + p.y + " " + p.z, p.x, p.y, p.z, maps[get_map_index(p.map)]);
			int x = hand.number(fixhand);
			send_reliable(p, "hempty " + x, 0);
			p.inv_add_item("skillet", 1);
			p.inv_add_item(fixhand, -1);
			return;
		} else if (fixhand.starts_with("package_of_") && hand.single) {
