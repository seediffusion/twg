		f.open(charfolder + "/lines.usr", "r");
		string[] data = string_split(f.read(), "\n");
		f.close();
		if (data.length()> 11) {
			p.editline = data[0];
			p.removeline = data[1];
			p.removeowner = data[2];
			p.aieditline = data[3];
