double get_closest_value(double num, int[] arr) {
	double diff = 0;
	double result;
	for (uint i = 0; i < arr.length(); i++) {
		int d = absolute(num - arr[i]);
		if (d < diff or diff == 0) {
			diff = d;
			result = arr[i];
		}
	}
	return result;
}
