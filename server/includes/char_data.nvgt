		p.mapavoltar = data;
		f.close();
	}
	if (file_exists(charfolder + "/playtime.usr")) {
		f.open(charfolder + "/playtime.usr", "r");
		p.playtimer.force(string_to_number(f.read()));
		f.close();
	}
