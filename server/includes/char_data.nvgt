const string charex = ".usr";
string get_charval(string name, string value, string ex = charex, string def = "") {
	name = verify_charname_case(name);
	string cf = "chars/" + name;
	string ret;
	file c;
	c.open(cf + "/" + value + ex, "r");
	ret = c.read();
	c.close();
	if (ret == "") ret = def;
	return ret;
}
string get_char_key_val(string name, string fn, string section, string key, string ex = charex, string def = "") {
	json t;
	t.loads(get_charval(name, fn, ex));
