	if (section != "") k.insert(0, section + ".");
	return t(k, def);
}
bool writeto(string name, string fn, string value, string ex = charex) {
	name = verify_charname_case(name);
	return file_put("chars/" + name + "/" + fn + ex, value);
}
bool writeto(string name, string fn, string section, string key, var@ value, string ex = charex) {
	json t;
	if (!t.loads(get_charval(name, fn, ex))) return false;
	string k = key;
	if (section != "") k.insert(0, section + ".");
	if (!t.set(k, value)) return false;
	return writeto(name, fn, t.dump(2), charex);
}
string verify_charname_case(string name) {
	string[] d = find_directories("chars/*");
	if (d.length() < 1) return name;
	for (uint i = 0; i < d.length(); i++) {
		if (name.lower() == d[i].lower()) name = d[i];
	}
	return name;
}
